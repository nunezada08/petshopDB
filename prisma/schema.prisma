generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model pet {
  id            Int   @id   @default(autoincrement())
  nome          String
  especie       String
  idade         Int
  dono          String

  @@map("pets") 
}